<%# locals: matches %>

<div class="external-entity-search-results">
  <% if matches.count.positive? %>
    <% matches.each do |match| %>
      <%= render partial: 'shared/entity_matcher/potential_match', locals: { match: match } %>
    <% end %>
  <% else %>
    <p><b>No potential matches found</b></p>
  <% end %>
</div>
