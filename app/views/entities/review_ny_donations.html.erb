<%= render partial: 'header', locals: { entity: @entity } %>
<%= render partial: 'actions', locals: { entity: @entity, current_user: current_user } %>

<div class="row mt-3 mb-3">
  <div class="col-12">
    <h2>Review New York State Political Donations</h2>
  </div>
</div>

<div class="row mt-2">
  <div class="col-md-6 col-sm-6">
    <p>Below are donor records from <a href="http://NYS">NYS Board of Elections</a> with the same or similar name.</p>
  </div>
  <div class="col-md-2 col-sm-2"></div>
</div>
<div class="row top-1em">
  <div class="col-md-10 col-sm-10">
    <div id="match-donations" data-entityid="<%= @entity.id %>">
      <table id="donations-table" class="display" width="100%"></table>
    </div>
    <script>new LittleSis.NyDonationsMatcher('unmatch', <%= @entity.id %> ).init();</script>
  </div>
  <div class="col-md-2 col-sm-2"></div>
</div>

</div>

