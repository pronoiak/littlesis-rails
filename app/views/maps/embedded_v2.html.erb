<%= content_for(:head) do %>
  <%= oligrapher_js_tags %>
<% end %>

<%= cache_if !user_signed_in?, [ 'embedded', @map, @start_index ] do %>

  <% content_for(:page_title, raw(@map.name)) %>
  <% content_for(:hide_footer, true) %>

  <%= content_tag(
    :div,
    id: "oligrapher",
    data: {
      controller: "oligrapher",
      oligrapher_config_value: @configuration.to_json
    }
  ){} %>
<% end %>
